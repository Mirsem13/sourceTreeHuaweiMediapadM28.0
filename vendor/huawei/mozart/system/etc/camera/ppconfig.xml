	</Mode>
	<Mode name="burstshot" switcher="1">
		<PPAlgo name="formatconvert" />
		<PPAlgo name="davinci" />
		<PPAlgo name="formatconvert" />
	</Mode>
	<Mode name="bestshot" switcher="1">
		<PPAlgo name="formatconvert" />
		<PPAlgo name="davinci" />
